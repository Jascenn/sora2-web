{
  "version": 2,
  "name": "sora2",
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ],
  "installCommand": "pnpm install",
  "buildCommand": "pnpm build",
  "framework": "nextjs",
  "outputDirectory": "apps/web/.next",
  "rootDirectory": "apps/web",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://sora2-web.vercel.app/api",
    "NEXT_PUBLIC_APP_URL": "https://sora2-web.vercel.app"
  }
}